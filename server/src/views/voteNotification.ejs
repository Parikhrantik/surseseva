<body class="clean-body u_body" style="margin: 0;padding: 0;-webkit-text-size-adjust: 100%;background-color: #f9f9f9;color: #000000">
    <table id="u_body" style="border-collapse: collapse;table-layout: fixed;border-spacing: 0;vertical-align: top;min-width: 320px;Margin: 0 auto;background-color: #f9f9f9;width:100%" cellpadding="0" cellspacing="0">
        <tbody>
            <tr style="vertical-align: top">
                <td style="word-break: break-word;border-collapse: collapse !important;vertical-align: top">

                    <!-- Header -->
                    <div class="u-row-container" style="padding: 0px;background-color: #ffffff">
                        <div class="u-row" style="margin: 0 auto;min-width: 320px;max-width: 600px;overflow-wrap: break-word;word-wrap: break-word;word-break: break-word;background-color: #172b4d;">
                            <div style="border-collapse: collapse;display: table;width: 100%;height: 100%;">
                                <div class="u-col u-col-100" style="max-width: 320px;min-width: 600px;display: table-cell;vertical-align: top;">
                                    <div style="height: 100%;width: 100% !important;">
                                        <div style="box-sizing: border-box; height: 100%; padding: 0px;">
                                            <table style="font-family:'Cabin',sans-serif;" role="presentation" cellpadding="0" cellspacing="0" width="100%" border="0">
                                                <tbody>
                                                    <tr>
                                                        <td style="overflow-wrap:break-word;word-break:break-word;padding:1px;font-family:'Cabin',sans-serif;" align="left">
                                                            <table width="100%" cellpadding="0" cellspacing="0" border="0">
                                                                <tr>
                                                                    <td style="padding: 20px;" align="center">
                                                                        <img align="center" border="0" src="/api/placeholder/200/80" alt="Logo" title="Logo" style="outline: none;text-decoration: none;-ms-interpolation-mode: bicubic;clear: both;display: inline-block !important;border: none;height: auto;float: none;width: 26%;max-width: 155.48px;" width="155.48"/>
                                                                    </td>
                                                                </tr>
                                                            </table>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Main Content -->
                    <div class="u-row-container" style="padding: 0px;background-color: #ffffff">
                        <div class="u-row" style="margin: 0 auto;min-width: 320px;max-width: 600px;overflow-wrap: break-word;word-wrap: break-word;word-break: break-word;background-color: #ffffff;">
                            <div style="border-collapse: collapse;display: table;width: 100%;height: 100%;">
                                <div class="u-col u-col-100" style="max-width: 320px;min-width: 600px;display: table-cell;vertical-align: top;">
                                    <div style="height: 100%;width: 100% !important;">
                                        <div style="box-sizing: border-box; height: 100%; padding: 0px;border: 2px solid #172b4d;">
                                            
                                            <!-- Vote Content -->
                                            <div style="padding: 40px 20px; text-align: center;">
                                                <h2 style="margin: 0; color: #172b4d; font-family: 'Cabin',sans-serif;">ðŸŽ‰ New Vote Received!</h2>
                                                <p style="margin: 20px 0; color: #172b4d; font-family: 'Cabin',sans-serif;">Dear <%= participantName %>,</p>
                                                <p style="margin: 20px 0; color: #172b4d; font-family: 'Cabin',sans-serif;">You have received a new vote in the competition!</p>
                                                
                                                <div style="background-color: #f8f9fa; padding: 20px; margin: 30px 0; border-left: 4px solid #172b4d;">
                                                    <p style="color: #172b4d; font-family: 'Cabin',sans-serif; font-weight: bold; margin-bottom: 10px;">Feedback:</p>
                                                    <p style="color: #172b4d; font-family: 'Cabin',sans-serif;"><%= voterFeedback %></p>
                                                </div>
                                                
                                                <p style="margin: 20px 0; color: #172b4d; font-family: 'Cabin',sans-serif;">Keep up the great work!</p>
                                            </div>
                                            
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Footer -->
                    <div class="u-row-container" style="padding: 0px;background-color: #ffffff">
                        <div class="u-row" style="margin: 0 auto;min-width: 320px;max-width: 600px;overflow-wrap: break-word;word-wrap: break-word;word-break: break-word;background-color: #172b4d;">
                            <div style="border-collapse: collapse;display: table;width: 100%;height: 100%;">
                                <div class="u-col u-col-100" style="max-width: 320px;min-width: 600px;display: table-cell;vertical-align: top;">
                                    <div style="height: 100%;width: 100% !important;">
                                        <div style="box-sizing: border-box; height: 100%; padding: 20px;">
                                            <p style="font-size: 14px; color: #fafafa; text-align: center; font-family: 'Cabin',sans-serif;">
                                                <span style="font-size: 16px;">Surseva</span>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </td>
            </tr>
